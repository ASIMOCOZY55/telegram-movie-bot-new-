    {
      "builds": [
        {
          "src": "index.py",
          "use": "@vercel/python",
          "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.9",
            "memory": 1024,        <-- MOVED HERE
            "maxDuration": 10      <-- MOVED HERE
          }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "index.py"
        }
      ],
      "installCommand": "pip install -r requirements.txt"
      // REMOVED THE "functions" BLOCK FROM HERE
    }
