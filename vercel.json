    {
      "builds": [
        {
          "src": "index.py",
          "use": "@vercel/python",
          "config": {
            "memory": 1024,
            "maxDuration": 10
          }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "index.py"
        }
      ],
      "installCommand": "pip install -r requirements.txt"
    }
